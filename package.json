{
  "name": "webpack-config",
  "version": "1.0.0",
  "description": "Webpack Config",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=dev PORT=3300 webpack-dev-server",
    "build:prod": "cross-env NODE_ENV=prod webpack -p",
